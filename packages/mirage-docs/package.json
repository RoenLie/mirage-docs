{
	"name": "@roenlie/mirage-docs",
	"version": "1.3.0",
	"description": "",
	"author": "Kristoffer Roen-Lie",
	"license": "MIT",
	"repository": "github/roenlie/mirage-docs",
	"bugs": {
		"url": "https://github.com/RoenLie/mirage-docs/issues",
		"email": "KristofferRoenLie@gmail.com"
	},
	"type": "module",
	"exports": {
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		},
		"./app": "./src/app/index.ts",
		"./app/*": "./src/app/*",
		"./assets/*": "./src/assets/*",
		"./workers/*": "./dist/workers/*"
	},
	"scripts": {
		"build": "pnpm build-server && pnpm build-workers && tsc --project ./src/server/tsconfig.json",
		"build-server": "vite build --config ./vite.config.ts",
		"build-workers": "vite build --config ./vite-workers.config.ts",
		"test": "vitest run --config ./tests/vite.config.ts",
		"updatedeps": "pnpm update --interactive",
		"increment-version": "pnpm mimic increment-version"
	},
	"dependencies": {
		"@custom-elements-manifest/analyzer": "^0.9.0",
		"@monaco-editor/loader": "^1.4.0",
		"@orama/orama": "2.0.0-beta.10",
		"@orama/plugin-data-persistence": "2.0.0-beta.10",
		"@orama/plugin-match-highlight": "2.0.0-beta.10",
		"@orama/plugin-parsedoc": "2.0.0-beta.10",
		"@roenlie/lit-aegis": "^1.0.21",
		"@sindresorhus/slugify": "^2.2.1",
		"deepmerge-ts": "^5.1.0",
		"globby": "^14.0.0",
		"highlight.js": "^11.9.0",
		"markdown-it": "^14.0.0",
		"markdown-it-anchor": "^8.6.7",
		"monaco-editor": "^0.45.0",
		"rollup-plugin-copy": "^3.5.0",
		"tabbable": "^6.2.0"
	},
	"peerDependencies": {
		"lit": "^3.1.0",
		"tslib": "^2.6.2",
		"vite": "^5.0.10"
	},
	"devDependencies": {
		"@types/markdown-it": "^13.0.7",
		"@types/sharedworker": "^0.0.110",
		"bootstrap-icons": "^1.11.2"
	}
}