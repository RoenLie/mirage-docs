{
	"name": "@roenlie/mirage-docs",
	"version": "2.0.0",
	"description": "",
	"author": "Kristoffer Roen-Lie",
	"license": "MIT",
	"repository": "github/roenlie/mirage-docs",
	"bugs": {
		"url": "https://github.com/RoenLie/mirage-docs/issues",
		"email": "KristofferRoenLie@gmail.com"
	},
	"type": "module",
	"files": [
		"./dist",
		"./src",
		"./LICENSE",
		"./package.json",
		"./README.md"
	],
	"exports": {
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		},
		"./app/*": "./dist/app/*",
		"./workers/*": "./dist/workers/*",
		"./assets/*": "./src/assets/*"
	},
	"scripts": {
		"build": "pnpm build-server && pnpm build-workers",
		"build-server": "vite build --config ./vite.config.ts && pnpm emit-types:server && pnpm emit-types:app",
		"build-workers": "vite build --config ./vite-workers.config.ts",
		"emit-types:app": "tsc --project ./src/app/tsconfig.json",
		"emit-types:server": "tsc --project ./src/server/tsconfig.json",
		"test": "vitest run --config ./tests/vite.config.ts",
		"updatedeps": "pnpm update --interactive",
		"increment-version": "package-toolbox increment-version",
		"package:local": "pnpm pack --pack-destination ../../_packages/"
	},
	"dependencies": {
		"@custom-elements-manifest/analyzer": "^0.9.0",
		"@monaco-editor/loader": "^1.4.0",
		"@orama/orama": "2.0.0-beta.11",
		"@orama/plugin-data-persistence": "2.0.0-beta.11",
		"@orama/plugin-match-highlight": "2.0.0-beta.11",
		"@orama/plugin-parsedoc": "2.0.0-beta.11",
		"@roenlie/lit-aegis": "^1.0.21",
		"@sindresorhus/slugify": "^2.2.1",
		"deepmerge-ts": "^5.1.0",
		"globby": "^14.0.0",
		"highlight.js": "^11.9.0",
		"markdown-it": "^14.0.0",
		"markdown-it-anchor": "^8.6.7",
		"monaco-editor": "^0.45.0",
		"rollup-plugin-copy": "^3.5.0",
		"tabbable": "^6.2.0",
		"tslib": "^2.6.2"
	},
	"peerDependencies": {
		"lit": "^3.1.0",
		"vite": "^5.0.10"
	},
	"devDependencies": {
		"@types/markdown-it": "^13.0.7",
		"@types/sharedworker": "^0.0.110",
		"bootstrap-icons": "^1.11.2",
		"lit": "^3.1.0",
		"vite": "^5.0.10",
		"vitest": "^1.0.4"
	},
	"sideEffects": true
}